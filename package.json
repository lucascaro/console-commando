{
  "name": "console-commando",
  "version": "6.0.0-beta.9",
  "description": "A tool for creating console commands",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test-watch": "mocha --require ts-node/register --watch-extensions ts,tsx 'test/**/*.{ts,tsx}'",
    "compile": "npm run clean && tsc -p .",
    "rollup": "rollup -c rollup.config.js",
    "watch": "tsc -w -p .",
    "docs": "typedoc --out docs src",
    "prepublish-only": "npm run test && npm run compile && npm run rollup",
    "release": "npm run compile && npm run test && npm version patch && git push --follow-tags"
  },
  "author": "Lucas Caro <lucascaro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lucascaro/console-commando.git"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.17",
    "@types/minimist": "^1.1.29",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "immutable": "^4.0.0-rc.12",
    "minimist": "^1.2.0",
    "typedoc": "^0.15.0"
  }
}
