{
  "name": "console-commando",
  "version": "3.1.10",
  "description": "A tool for creating console commands",
  "main": "./lib/Commando.js",
  "types": "./lib/Commando.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "mocha --require ts-node/register 'test/**/*.{ts,tsx}'",
    "test-watch": "mocha --require ts-node/register --watch-extensions ts,tsx 'test/**/*.{ts,tsx}'",
    "compile": "npm run clean && tsc -p ./tsconfig-build.json",
    "watch": "tsc -w -p ./tsconfig-build.json",
    "docs": "esdoc -c esdoc.json",
    "prepare": "npm run compile",
    "release": "npm run compile && npm run test && npm version patch && git push --follow-tags"
  },
  "author": "Lucas Caro <lucascaro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lucascaro/console-commando.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/expect.js": "^0.3.29",
    "@types/immutable": "^3.8.4",
    "@types/minimist": "^1.1.29",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.7",
    "@types/sinon": "^5.0.5",
    "@types/strip-ansi": "^3.0.0",
    "esdoc": "^1.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "sinon": "^6.3.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "immutable": "^3.8.2",
    "minimist": "^1.2.0",
    "string.prototype.repeat": "^0.2.0",
    "strip-ansi": "^5.0.0",
    "ts-polyfill": "^3.0.1"
  }
}
