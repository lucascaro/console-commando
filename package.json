{
  "name": "console-commando",
  "version": "6.0.4",
  "description": "A tool for creating console commands",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "check": "tsc -p . --noEmit",
    "compile": "npm run clean && npm run rollup",
    "docs": "rm -rf docs && typedoc --out docs src && mkdir -p versions/`npm run --silent version` && cp -r docs/* $_ && mv versions docs",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run test && npm run compile",
    "prerelease": "npm run compile && npm run test && npm version prerelease && git push --follow-tags",
    "release": "npm run compile && npm run test && npm version patch && git push --follow-tags",
    "rollup": "rollup -c rollup.config.js",
    "test:watch": "jest --watch",
    "test": "jest",
    "version": "echo $npm_package_version",
    "watch": "rollup -c rollup.config.js --watch"
  },
  "author": "Lucas Caro <lucascaro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lucascaro/console-commando.git"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/minimist": "^1.1.29",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "mock-stdin": "^0.3.1",
    "prettier": "^1.18.2",
    "rollup": "^1.21.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "immutable": "^4.0.0-rc.12",
    "minimist": "^1.2.0",
    "stdio-mock": "^1.1.0"
  }
}
