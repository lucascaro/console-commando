{
  "name": "console-commando",
  "version": "6.0.0-beta.1",
  "description": "A tool for creating console commands",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test-watch": "mocha --require ts-node/register --watch-extensions ts,tsx 'test/**/*.{ts,tsx}'",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "docs": "esdoc -c esdoc.json",
    "prepublish-only": "npm run test && npm run compile",
    "release": "npm run compile && npm run test && npm version patch && git push --follow-tags"
  },
  "author": "Lucas Caro <lucascaro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lucascaro/console-commando.git"
  },
  "devDependencies": {
    "@types/debug": "^4.1.2",
    "@types/jest": "^24.0.6",
    "@types/minimist": "^1.1.29",
    "@types/node": "^10.12.0",
    "@types/strip-ansi": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "esdoc": "^1.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "immutable": "^4.0.0-rc.12",
    "minimist": "^1.2.0"
  }
}
