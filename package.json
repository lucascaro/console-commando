{
  "name": "console-commando",
  "version": "3.1.6",
  "description": "A tool for creating console commands",
  "main": "./lib/commando.js",
  "types": "./lib/commando.d.ts",
  "scripts": {
    "clean": "rm -rf lib/*",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register",
    "test-watch": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --watch",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run compile",
    "prepare": "npm run compile",
    "release": "npm run compile && npm run test && npm version patch && git push --follow-tags"
  },
  "author": "Lucas Caro <lucascaro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lucascaro/console-commando.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/chalk": "^0.4.29",
    "@types/immutable": "^3.8.4",
    "@types/minimist": "^1.1.29",
    "@types/mocha": "^2.2.48",
    "@types/node": "^6.0.118",
    "esdoc": "^1.1.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "sinon": "^1.17.1",
    "ts-node": "^3.3.0",
    "tslint": "^3.15.1",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "immutable": "^3.8.2",
    "minimist": "^1.2.0",
    "string.prototype.repeat": "^0.2.0",
    "ts-polyfill": "^3.0.1"
  }
}
