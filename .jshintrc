{
  "browser": false,
  "node": true,
  "esnext": true,
  "globals": {
    "describe": true,
    "it": true
  },
  "globalstrict": true,
  "quotmark": "single",
  "smarttabs": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "indent": 2,
  "latedef": "nofunc",
  "shadow": true,
  "onevar": true,
  "maxlen": 80,
  "forin": true,
  "curly": true,
  "freeze": true,
  "newcap": true,
  "noarg": true,
  "nonew": true,
  "maxcomplexity": 7,
  "maxparams": 4,
  "maxdepth": 3,
  "strict": true
}
